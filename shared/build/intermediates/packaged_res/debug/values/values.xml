        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>
    <declare-styleable name="ArcProgress">
        <attr format="integer" name="arc_progress"/>
        <attr format="dimension" name="arc_stroke_width"/>
        <attr format="integer" name="arc_max"/>

        <attr format="color" name="arc_unfinished_color"/>
        <attr format="color" name="arc_finished_color"/>
        <attr format="color" name="arc_stroke_color"/>

        <attr format="string" name="arc_left_day"/>
        <attr format="dimension" name="arc_text_size"/>
        <attr format="color" name="arc_text_color"/>

        <attr format="string" name="arc_speed_text"/>
        <attr format="dimension" name="arc_speed_text_size"/>
        <attr format="color" name="arc_speed_text_color"/>

        <attr format="string" name="arc_package_text"/>
        <attr format="dimension" name="arc_package_text_size"/>
        <attr format="color" name="arc_package_text_color"/>
    </declare-styleable>
    <declare-styleable name="OtpView">
    <attr name="android:inputType"/>
    <attr name="android:textColor"/>
    <attr format="color" name="text_background_color"/>
    <attr format="string" name="otp"/>
    <attr format="integer" name="length"/>
    <attr format="dimension" name="width"/>
    <attr format="dimension" name="height"/>
    <attr format="dimension" name="space"/>
    <attr format="dimension" name="space_left"/>
    <attr format="dimension" name="space_right"/>
    <attr format="dimension" name="space_top"/>
    <attr format="dimension" name="space_bottom"/>
    <attr format="color" name="hint_color"/>
    <attr format="string" name="hint"/>
  </declare-styleable>
    <declare-styleable name="PinEntryView">
        
        <attr format="integer" name="numDigits"/>
        
        <attr format="dimension" name="digitWidth"/>
        <attr format="dimension" name="digitHeight"/>
        
        <attr format="dimension" name="digitSpacing"/>
        
        <attr format="reference|color" name="digitBackground"/>
        
        <attr format="dimension" name="digitTextSize"/>
        
        <attr format="color" name="digitTextColor"/>
        
        <attr format="dimension" name="digitElevation"/>
        
        <attr format="string" name="mask"/>
        
        <attr format="color" name="pinAccentColor"/>
        
        <attr format="dimension" name="accentWidth"/>
        
        <attr format="enum" name="accentType">
            <enum name="none" value="0"/>
            <enum name="all" value="1"/>
            <enum name="character" value="2"/>
        </attr>
        
        <attr format="enum" name="pinInputType">
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
        </attr>
        
        <attr format="boolean" name="accentRequiresFocus"/>
    </declare-styleable>
    <declare-styleable name="PinView">
        <attr format="integer" name="itemCount"/>
        <attr format="dimension" name="itemWidth"/>
        <attr format="dimension" name="itemHeight"/>
        <attr format="dimension" name="itemRadius"/>
        <attr format="dimension" name="itemSpacing"/>
        <attr format="dimension" name="lineWidth"/>
        <attr format="reference|color" name="lineColor"/>
        <attr format="enum" name="viewType">
            <enum name="rectangle" value="0"/>
            <enum name="line" value="1"/>
        </attr>
        <attr name="android:cursorVisible"/>
        <attr format="dimension" name="cursorWidth"/>
        <attr format="reference|color" name="cursorColor"/>
    </declare-styleable>
    <declare-styleable name="PinViewTheme">
        <attr format="reference" name="pinViewStyle"/>
    </declare-styleable>
    <declare-styleable name="Themes">
        <attr format="reference" name="arcProgressStyle"/>
    </declare-styleable>
