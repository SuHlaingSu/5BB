                                android:src="@drawable/ic_pencil"
                                app:layout_constraintBottom_toBottomOf="@id/ivCustomerInfoExpand"
                                app:layout_constraintStart_toEndOf="@id/lblCustomerInfo"
                                app:layout_constraintTop_toTopOf="@id/ivCustomerInfoExpand" />
-->
